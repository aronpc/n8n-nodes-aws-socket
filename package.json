{
  "name": "n8n-nodes-websocket-aws",
  "version": "1.1.4",
  "description": "n8n nodes to send and receive messages from AWS API Gateway WebSocket connections",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp nodes/AwsWebSocket/awsWebSocket.svg dist/nodes/AwsWebSocket/ && mkdir -p dist/nodes/AwsWebSocketTrigger && cp nodes/AwsWebSocket/awsWebSocket.svg dist/nodes/AwsWebSocketTrigger/",
    "dev": "tsc --watch",
    "test": "jest"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "aws",
    "websocket",
    "apigateway"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.400.0",
    "@aws-sdk/credential-provider-ini": "^3.400.0",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@types/ws": "^8.5.5",
    "jest": "^29.5.0",
    "typescript": "^5.0.4"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AwsWebSocketApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AwsWebSocket/AwsWebSocket.node.js",
      "dist/nodes/AwsWebSocketTrigger/AwsWebSocketTrigger.node.js"
    ]
  }
}
